<!--Header-->
<header>

  <!--Logo-->
  <!-- Dropdown  -->
  <ul id="dropdown1" class="dropdown-content">
    <li><%= session_link %></li>
    <li>TESTING/li>
    <li class="divider"></li>
    <li><a href="#!">three</a></li>
  </ul>

  <ul id="dropdown2" class="dropdown-content">
    <li><%= session_link %></li>
    <li><a href="#!">two</a></li>
    <li class="divider"></li>
    <li><a href="#!">three</a></li>
  </ul>

  <!--Navs-->
  <nav>
    <div class="container">
      <div class="nav-wrapper">
        <a href="#" class="brand-logo right">EPN</a>
        <% link_to "EPN", root_path, class: "brand-logo right" %>
        <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
        <ul id="nav-mobile" class="left hide-on-med-and-down">
          <li><% link_to "Cursos", root_path %></li>
          <% if logged_in? %>
            <li><%= link_to "Estudiantes", students_path %></li>
          <% end %>
          <li><a href="#">Inscripciones</a></li>
          <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">Cuenta<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>

        <ul class="sidenav" id="mobile-demo">
          <li><% link_to "Cursos", root_path %></li>
          <li><a href="#">Cursos</a></li>
          <li><a href="#">Talleres</a></li>
          <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">Cuenta<i class="material-icons right">arrow_drop_down</i></a></li>

        </ul>

      </div>
    </div>
  </nav>

  <% flash.each do |key, message| %>
    <div class="container">
      <div class="row sign-up-form">
        <div class="col s12 m12 l12">
          <div class="card-panel red lighten-2" id="esconde">
            <span class="white-text">
              <%= message %>
            </span>
          </div>
        </div>
      </div>
    </div>
  <% end %>

</header>